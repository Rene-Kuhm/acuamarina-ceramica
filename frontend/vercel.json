{
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "https://acuamarina-ceramica-rbqj.vercel.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}
